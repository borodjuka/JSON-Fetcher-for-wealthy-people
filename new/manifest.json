{
  "manifest_version": 2,

  "name": "Fetch JSON",
  "description": "This extension fetches JSON data from specific page.",
  "version": "1.0",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },  

  "browser_action": {
    "name": "Fetch JSON"
},

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },


  "permissions": ["tabs","downloads", "https://app.meinpflegedienst.com/*"]
}
